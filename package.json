{
  "name": "casperjs-assert-match-snapshot",
  "version": "1.0.0",
  "description": "casperjs tester extension to assert snapshots matched",
  "scripts": {
    "pretest": "eslint *.js test",
    "test:unit": "nyc --reporter=lcov --reporter=text mocha test/unit",
    "temp": "mocha --watch test/unit/index.test.js",
    "test:e2e": "node test/e2e/runner.js",
    "test": "npm run test:unit && npm run test:e2e"
  },
  "files": [
    "index.js",
    "options-helper.js",
    "option-error.js"
  ],
  "keywords": [
    "casper",
    "casperjs",
    "phantomjs",
    "slimerjs",
    "assert",
    "tester",
    "snapshot",
    "match"
  ],
  "author": "Roney Rao",
  "license": "MIT",
  "repository": "https://github.com/roneyrao/casperjs-assert-match-snapshot",
  "peerDependencies": {
    "casperjs": "^1.1.4"
  },
  "dependencies": {},
  "devDependencies": {
    "casperjs": "^1.1.4",
    "chai": "^4.1.2",
    "eslint": "^4.19.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "mocha": "^5.0.4",
    "mock-require": "^3.0.1",
    "npm-install-package": "^2.1.0",
    "nyc": "^11.6.0",
    "resemblejs": "^2.10.0",
    "rewire": "^3.0.2",
    "sinon": "^4.4.6",
    "sinon-chai": "^3.0.0"
  }
}
